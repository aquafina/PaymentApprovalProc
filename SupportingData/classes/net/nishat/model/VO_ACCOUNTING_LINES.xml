  <Variable
    Name="p_cdate_lo"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="p_cdate_hi"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="p_check_id"
    Kind="where"
    Type="oracle.jbo.domain.Number"
    DefaultValue="2317535"/>
  <SQLQuery>
    <![CDATA[SELECT  aca.CHECK_ID,
gcc.code_combination_id CODE_COMBO_ID,
       gcc.segment3 || '-' || gcc.segment4 code_commb,
       apps.get_acc_desc (gcc.code_combination_id) acc_desc,
       xel.entered_DR,
       xel.entered_CR,
       xel.ACCOUNTED_DR ACCOUNTED_DR,
       xel.ACCOUNTED_CR ACCOUNTED_CR,
       xel.CODE_COMBINATION_ID,
       xel.ACCOUNTING_DATE gl_date,
       xel.CURRENCY_CODE
  FROM xla_ae_lines xel,
       xla_ae_headers xeh,
       ap_checks_all aca,
       xla.xla_transaction_entities xte,
       po_vendors pv,
       po_vendor_sites_all pvsa,
       gl_code_combinations_kfv gcc
 WHERE     xte.application_id = 200
       AND xel.application_id = xeh.application_id
       AND xte.application_id = xeh.application_id
       AND xel.ae_header_id = xeh.ae_header_id
       AND xte.entity_code = 'AP_PAYMENTS'
       AND xel.code_combination_id = gcc.code_combination_id
       AND xel.ACCOUNTING_CLASS_CODE NOT IN ('INTRA')
       AND xte.source_id_int_1 = aca.CHECK_ID
       AND xte.entity_id = xeh.entity_id
       AND aca.VENDOR_ID = pv.VENDOR_ID(+)
       AND aca.VENDOR_SITE_ID = pvsa.VENDOR_SITE_ID(+)
       AND TO_DATE (aca.check_date) BETWEEN NVL (:p_cdate_lo, '01jan1995')
                            AND NVL (:p_cdate_hi, SYSDATE)
       AND aca.check_id = NVL (:p_check_id, aca.check_id)]]>
  </SQLQuery>
